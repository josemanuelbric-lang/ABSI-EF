    def construir_matriz_markov(self, n_max=3D1000):
        """Construye la matriz de transici=C3=B3n de Markov entre clases di=
mensionales"""
        clases =3D ["C0", "C1", "C2", "C3", "C4"]
        matriz =3D np.zeros((5, 5))
       =20
        # Analizar transiciones para muchos n=C3=BAmeros
        transiciones =3D self.analizar_flujo_dimensional(n_max)
       =20
        # Contar transiciones totales por clase origen
        conteo_origen =3D {clase: 0 for clase in clases}
        for (origen, destino), count in transiciones.items():
            if origen in clases:
                conteo_origen[origen] +=3D count
       =20
        # Construir matriz de probabilidades
        for (origen, destino), count in transiciones.items():
            if origen in clases and destino in clases:
                i =3D clases.index(origen)
                j =3D clases.index(destino)
                if conteo_origen[origen] &gt; 0:
                    matriz[i, j] =3D count / conteo_origen[origen]
       =20
        return matriz
