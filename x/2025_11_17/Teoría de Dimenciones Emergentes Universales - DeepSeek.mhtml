    def calcular_carga_dimensional(self, n):
        """Calcula =CF=86(n) =3D (=CF=89(n), =CE=BD=E2=82=82(n), =CE=A9(n))=
"""
        if n =3D=3D 1:
            return (0, 0, 0)  # Estado fundamental
           =20
        factores =3D factorint(n)
        omega =3D len([p for p in factores if p !=3D 2])
        nu2 =3D factores.get(2, 0)
        omega_total =3D sum(factores.values())
       =20
        return (omega, nu2, omega_total)
   =20
    def clasificar_numero(self, n):
        """Clasifica un n=C3=BAmero en su dimensi=C3=B3n correspondiente"""
        if n =3D=3D 1:
            return "C0"  # Estado Fundamental
           =20
        factores =3D factorint(n)
        primos_impares =3D [p for p in factores if p !=3D 2]
       =20
        # C2: Potencias puras de 2
        if not primos_impares and n &gt; 1:
            return "C2"
           =20
        # Es impar
        if n % 2 =3D=3D 1:
            if len(primos_impares) =3D=3D 1 and factores[primos_impares[0]]=
 =3D=3D 1:
                return "C1"  # Primo impar
            else:
