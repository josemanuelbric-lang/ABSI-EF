        else:
            # Es par pero no potencia pura de 2
            return "C4"  # Mixto par
   =20
    def aplicar_collatz(self, n):
        """Aplica una iteraci=C3=B3n de Collatz"""
        if n % 2 =3D=3D 0:
            return n // 2
        else:
            return 3 * n + 1
   =20
    def analizar_flujo_dimensional(self, n_max):
        """Analiza el flujo entre clases dimensionales"""
        transiciones =3D collections.defaultdict(int)
       =20
        for n in range(2, n_max + 1):
            clase_actual =3D self.clasificar_numero(n)
            siguiente =3D self.aplicar_collatz(n)
            clase_siguiente =3D self.clasificar_numero(siguiente)
           =20
            # Registrar transici=C3=B3n dimensional
            key =3D (clase_actual, clase_siguiente)
            transiciones[key] =3D transiciones[key] + 1
           =20
        return transiciones
