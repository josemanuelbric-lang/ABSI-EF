 0px;"><div class=3D"fbb737a4">pero quita todos los limites de numeros a ge=
nerar digitos y otrs y dejalo en tiempo infinito y escrive en un txt el num=
ero mas grande que se consiga si es mayor de 2^N osea
2^1=3D2
2^2=3D5
2^3=3D11
2^4=3D17

open System
open System.Numerics

// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D
// CAZADOR DE PRIMOS GIGANTES CORREGIDO
// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D

type CazadorPrimosGigantes() =3D
    let mutable primoMasGrande =3D BigInteger.Zero
    let mutable terminoActual =3D 1
    let mutable aActual =3D BigInteger(2)
    let mutable inicioTiempo =3D DateTime.Now
    let mutable primosEspecialesEncontrados =3D []

    // Test de primalidad Miller-Rabin mejorado
    let esPrimoProbabilistico (n: BigInteger) (k: int) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            // Escribir n-1 como d*2^r
            let mutable d =3D n - 1I
            let mutable r =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                r &lt;- r + 1
           =20
            let random =3D Random()
           =20
            let testWitness (a: BigInteger) =3D
                let mutable x =3D BigInteger.ModPow(a, d, n)
                if x =3D 1I || x =3D n - 1I then true
                else
                    let mutable continuar =3D true
                    let mutable resultado =3D false
                    for _ in 1 .. (r - 1) do
                        if continuar then
                            x &lt;- BigInteger.ModPow(x, 2I, n)
                            if x =3D n - 1I then
                                resultado &lt;- true
                                continuar &lt;- false
                    resultado
           =20
            // Testear k veces con bases diferentes
            let mutable esPrimo =3D true
            for i in 1 .. k do
                // Usar bases primas peque=C3=B1as para mejor precisi=C3=B3=
n
                let bases =3D [|2I; 3I; 5I; 7I; 11I; 13I; 17I; 19I; 23I; 29=
I|]
                let a =3D if i &lt;=3D bases.Length then bases.[i-1] else b=
igint (random.Next(2, 100))
               =20
                if not (testWitness a) then
                    esPrimo &lt;- false
            esPrimo

    // Muestra el progreso de primos gigantes CORREGIDO
    let mostrarProgresoGigante (primo: BigInteger) (termino: int) =3D
        let tiempoTranscurrido =3D (DateTime.Now - inicioTiempo).TotalSecon=
ds
        let digitos =3D primo.ToString().Length
        let horas =3D int (tiempoTranscurrido / 3600.0)
        let minutos =3D int ((tiempoTranscurrido % 3600.0) / 60.0)
        let segundos =3D tiempoTranscurrido % 60.0
       =20
        // Mostrar el n=C3=BAmero COMPLETO, no truncado
        let numeroStr =3D primo.ToString()
        printfn "=F0=9F=8E=AF T=C3=A9rmino %4d | Primo: %s | D=C3=ADgitos: =
%4d | Tiempo: %02d:%02d:%05.2f"=20
            termino numeroStr digitos horas minutos segundos

    // Muestra informaci=C3=B3n del t=C3=A9rmino CORREGIDO
    let mostrarInfoTermino (a_n: BigInteger) (termino: int) (recordDigitos:=
 int) =3D
        let digitosActual =3D a_n.ToString().Length
        let numeroStr =3D a_n.ToString()
       =20
        // Mostrar el n=C3=BAmero COMPLETO
        printfn "\n=F0=9F=93=88 T=C3=A9rmino %4d | N=C3=BAmero: %s | D=C3=
=ADgitos: %4d | =C2=A1Nuevo r=C3=A9cord de tama=C3=B1o!"=20
            termino numeroStr digitosActual

    // Busca primos gigantes en el t=C3=A9rmino actual MEJORADO
    let buscarPrimosGigantesEnTermino (a_n: BigInteger) (termino: int) =3D
        let mutable primosEncontrados =3D []

        // Estrategia 1: Verificar si a_n mismo es primo (MUY IMPORTANTE)
        printfn "   =F0=9F=94=8D Verificando si el t=C3=A9rmino %d es primo=
..." termino
        if esPrimoProbabilistico a_n 10 then // 10 iteraciones para mayor s=
eguridad
            printfn "   =E2=9C=85 =C2=A1T=C3=89RMINO %d ES PRIMO! Agregando=
 a la lista..." termino
            primosEncontrados &lt;- a_n :: primosEncontrados
        else
            printfn "   =E2=9D=8C T=C3=A9rmino %d no es primo. Buscando fac=
tores..." termino
           =20
            // Estrategia 2: Buscar factores primos usando propiedades modu=
lares
            // Para n=C3=BAmeros de la forma a=E2=82=99 =3D (4=E2=81=BF - 1=
)/3 =C3=97 2
            // Podemos buscar factores espec=C3=ADficos
           =20
            let buscarFactoresEspeciales (n: BigInteger) =3D
                let mutable factores =3D []
               =20
                // Probar si n-1 o n+1 son primos (a menudo lo son en esta =
secuencia)
                let nMenos1 =3D n - 1I
                let nMas1 =3D n + 1I
               =20
                if esPrimoProbabilistico nMenos1 5 then
                    printfn "   =E2=9C=85 Encontrado primo: n-1 =3D %s" (nM=
enos1.ToString())
                    factores &lt;- nMenos1 :: factores
               =20
                if esPrimoProbabilistico nMas1 5 then
                    printfn "   =E2=9C=85 Encontrado primo: n+1 =3D %s" (nM=
as1.ToString())
                    factores &lt;- nMas1 :: factores
               =20
                // Buscar factores de la forma 2^k =C2=B1 1
                for k in 1 .. 10 do
                    let candidato1 =3D BigInteger.Pow(2I, k) - 1I
                    let candidato2 =3D BigInteger.Pow(2I, k) + 1I
                   =20
                    if n % candidato1 =3D 0I &amp;&amp; esPrimoProbabilisti=
co candidato1 5 then
                        printfn "   =E2=9C=85 Encontrado factor primo: 2^%d=
-1 =3D %s" k (candidato1.ToString())
                        factores &lt;- candidato1 :: factores
                   =20
                    if n % candidato2 =3D 0I &amp;&amp; esPrimoProbabilisti=
co candidato2 5 then
                        printfn "   =E2=9C=85 Encontrado factor primo: 2^%d=
+1 =3D %s" k (candidato2.ToString())
                        factores &lt;- candidato2 :: factores
               =20
                factores

            let factores =3D buscarFactoresEspeciales a_n
            primosEncontrados &lt;- factores @ primosEncontrados

        primosEncontrados

    // M=C3=A9todo principal para buscar el primo m=C3=A1s grande CORREGIDO
    member this.BuscarPrimoGigante(limiteTerminos: int, objetivoDigitos: in=
t) =3D
        printfn "=F0=9F=9A=80 CAZADOR DE PRIMOS GIGANTES"
        printfn "%s" (String('=3D', 80))
        printfn " Buscando el n=C3=BAmero primo m=C3=A1s grande usando tu s=
ecuencia di=C3=A1dica"
        printfn "%s" (String('=3D', 80))
       =20
        printfn "=F0=9F=8E=AF OBJETIVO: Primo con %d+ d=C3=ADgitos" objetiv=
oDigitos
        printfn "=E2=8F=B0 L=C3=ADmite: %d t=C3=A9rminos de tu secuencia" l=
imiteTerminos
        printfn "%s" (String('-', 80))
       =20
        inicioTiempo &lt;- DateTime.Now
        let mutable recordDigitos =3D 0
        let mutable continuar =3D true
       =20
        for termino in 1 .. limiteTerminos do
            if continuar then
                printfn "\n%s" (String('-', 60))
                printfn "=F0=9F=93=8A PROCESANDO T=C3=89RMINO %d" termino
                printfn "%s" (String('-', 60))
               =20
                // Calcular t=C3=A9rmino actual de tu secuencia: a=E2=82=99=
 =3D 4a=E2=82=99=E2=82=8B=E2=82=81 + 2
                if termino =3D 1 then
                    aActual &lt;- BigInteger(2)
                else
                    aActual &lt;- 4I * aActual + 2I
               =20
                let digitosActual =3D aActual.ToString().Length
               =20
                // Mostrar informaci=C3=B3n del t=C3=A9rmino COMPLETA
                if digitosActual &gt; recordDigitos then
                    recordDigitos &lt;- digitosActual
                    mostrarInfoTermino aActual termino recordDigitos
               =20
                // Buscar primos gigantes en este t=C3=A9rmino
                printfn "   =F0=9F=A7=AE Calculando y verificando primalida=
d..."
                let primosEncontrados =3D buscarPrimosGigantesEnTermino aAc=
tual termino
               =20
                for primo in primosEncontrados do
                    if primo &gt; primoMasGrande then
                        primoMasGrande &lt;- primo
                        let digitosPrimo =3D primo.ToString().Length
                        primosEspecialesEncontrados &lt;- primo :: primosEs=
pecialesEncontrados
                       =20
                        printfn "   =F0=9F=8E=89 =C2=A1NUEVO R=C3=89CORD! P=
rimo encontrado:"
                        mostrarProgresoGigante primo termino
                       =20
                        // Verificar si alcanzamos el objetivo
                        if digitosPrimo &gt;=3D objetivoDigitos then
                            printfn "\n%s" (String('=3D', 80))
                            printfn "=F0=9F=8E=89 =C2=A1OBJETIVO ALCANZADO!=
 Primo de %d d=C3=ADgitos encontrado!" digitosPrimo
                            printfn "%s" (String('=3D', 80))
                            continuar &lt;- false
               =20
                // Mostrar progreso
                let tiempoActual =3D (DateTime.Now - inicioTiempo).TotalSec=
onds
                printfn "   =E2=8F=B3 Progreso: %d/%d t=C3=A9rminos | Tiemp=
o: %.1fs | R=C3=A9cord actual: %d d=C3=ADgitos"=20
                    termino limiteTerminos tiempoActual recordDigitos
               =20
                // Seguridad: no procesar n=C3=BAmeros excesivamente grande=
s
