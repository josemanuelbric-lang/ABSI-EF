        100;      // 2^1,000,000  (~301,030 d=C3=ADgitos)
        1000;      // 2^1,000,000  (~301,030 d=C3=ADgitos)
        10000;      // 2^2,000,000  (~602,060 d=C3=ADgitos)=20
