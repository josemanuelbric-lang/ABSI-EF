// CAZADOR DE PRIMOS - CON PRIMALIDAD MEJORADA
// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D

type CazadorPrimosDesdeUno() =3D
    let mutable primoMasGrande =3D BigInteger.Zero
    let mutable terminoActual =3D 1
    let mutable aActual =3D BigInteger(2)  // Comenzar desde a=E2=82=81 =3D=
 2
    let mutable inicioTiempo =3D DateTime.Now
    let archivoResultados =3D "primos_desde_uno.txt"
   =20
    // Objetivos de crecimiento =C3=9710
    let objetivosDigitos =3D [|
        1;          // 1 d=C3=ADgito
        10;         // 10 d=C3=ADgitos =20
        100;        // 100 d=C3=ADgitos
        1_000;      // 1,000 d=C3=ADgitos
        10_000;     // 10,000 d=C3=ADgitos
        100_000;    // 100,000 d=C3=ADgitos
        1_000_000;  // 1 mill=C3=B3n d=C3=ADgitos
        10_000_000; // 10 millones d=C3=ADgitos
        41_024_320  // R=C3=A9cord actual
    |]

    // Crear archivo de resultados
    do
        if not (File.Exists(archivoResultados)) then
            File.WriteAllText(archivoResultados,=20
                "=3D=3D=3D CAZADOR DE PRIMOS - DESDE 1 Y CRECE =C3=9710 =3D=
=3D=3D\n" +
                "Fecha de inicio: " + DateTime.Now.ToString() + "\n" +
                "Secuencia: a=E2=82=99 =3D 4a=E2=82=99=E2=82=8B=E2=82=81 + =
2, comenzando desde a=E2=82=81 =3D 2\n" +
                "Crecimiento: 1 =E2=86=92 10 =E2=86=92 100 =E2=86=92 1,000 =
=E2=86=92 10,000 =E2=86=92 ... d=C3=ADgitos\n" +
                "=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D\n\n")

    // Test de primalidad Miller-Rabin mejorado
    let esPrimoMillerRabin (n: BigInteger) (k: int) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            // Escribir n-1 como 2^s * d
            let mutable d =3D n - 1I
            let mutable s =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                s &lt;- s + 1
           =20
            let random =3D Random()
           =20
            let testWitness (a: BigInteger) =3D
                let mutable x =3D BigInteger.ModPow(a, d, n)
                if x =3D 1I || x =3D n - 1I then
                    true
                else
                    let mutable continuar =3D true
                    let mutable esTestigo =3D false
                    let mutable r =3D 1
                    while r &lt; s &amp;&amp; continuar do
                        x &lt;- BigInteger.ModPow(x, 2I, n)
                        if x =3D n - 1I then
                            esTestigo &lt;- true
                            continuar &lt;- false
                        r &lt;- r + 1
                    esTestigo
           =20
            // Usar bases determin=C3=ADsticas para n=C3=BAmeros peque=C3=
=B1os
            let bases =3D=20
                if n &lt; 1373653I then [|2I; 3I|]
                elif n &lt; 9080191I then [|31I; 73I|]
                elif n &lt; 4759123141I then [|2I; 7I; 61I|]
                elif n &lt; 2152302898747I then [|2I; 3I; 5I; 7I; 11I|]
                elif n &lt; 3474749660383I then [|2I; 3I; 5I; 7I; 11I; 13I|=
]
                else [|2I; 3I; 5I; 7I; 11I; 13I; 17I|] // Para n=C3=BAmeros=
 grandes
           =20
            let mutable esPrimo =3D true
            let mutable i =3D 0
           =20
            while i &lt; min bases.Length k &amp;&amp; esPrimo do
                let a =3D bases.[i]
                if a &lt; n then
                    if not (testWitness a) then
                        esPrimo &lt;- false
                i &lt;- i + 1
           =20
            esPrimo

    // Test r=C3=A1pido para screening inicial
    let esPosiblePrimo (n: BigInteger) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            // Test r=C3=A1pido con divisi=C3=B3n por peque=C3=B1os primos
            let peque=C3=B1osPrimos =3D [|3I; 5I; 7I; 11I; 13I; 17I; 19I; 2=
3I; 29I|]
            let mutable i =3D 0
            let mutable esPrimo =3D true
           =20
            while i &lt; peque=C3=B1osPrimos.Length &amp;&amp; esPrimo do
                if n % peque=C3=B1osPrimos.[i] =3D 0I then
                    esPrimo &lt;- false
                i &lt;- i + 1
           =20
            if esPrimo then
                // Si pasa el test r=C3=A1pido, hacer Miller-Rabin con poca=
s iteraciones
                esPrimoMillerRabin n 3
            else
                false

    // B=C3=BAsqueda m=C3=A1s exhaustiva de primos cercanos
    let buscarPrimosCercanos (centro: BigInteger) =3D
        printfn "   =F0=9F=94=8D Probando candidatos alrededor del t=C3=A9r=
mino..."
        let mutable encontrados =3D []
       =20
        // Probar m=C3=A1s offsets para aumentar probabilidades
        let offsets =3D [|
            -9; -7; -5; -3; -1;=20
            1; 3; 5; 7; 9;
            -15; -13; -11;=20
            11; 13; 15;
            -21; -19; -17;
            17; 19; 21
        |]
       =20
        let mutable probados =3D 0
        let mutable primosEncontrados =3D 0
       =20
        for offset in offsets do
            let candidato =3D centro + BigInteger(offset)
            if candidato &gt; 1I then
                probados &lt;- probados + 1
               =20
                // Filtro r=C3=A1pido
                if candidato % 2I &lt;&gt; 0I &amp;&amp; candidato % 3I &lt=
;&gt; 0I then
                    if probados % 5 =3D 0 then
                        printfn "   =F0=9F=93=8A Probado %d/%d candidatos..=
." probados offsets.Length
                   =20
                    if esPosiblePrimo candidato then
                        // Confirmar con m=C3=A1s tests
                        if esPrimoMillerRabin candidato 5 then
                            primosEncontrados &lt;- primosEncontrados + 1
                            encontrados &lt;- candidato :: encontrados
                            printfn "   =F0=9F=8E=89 =C2=A1PRIMO CONFIRMADO=
! %s d=C3=ADgitos"=20
                                (candidato.ToString().Length.ToString("N0")=
)
       =20
        printfn "   =F0=9F=93=8A Resultado: %d primos de %d candidatos prob=
ados" primosEncontrados probados
        encontrados

    // Avance hasta alcanzar objetivo de d=C3=ADgitos
    let avanzarHastaObjetivo (objetivoDigitos: int) =3D
        printfn "\n=F0=9F=8E=AF OBJETIVO: Alcanzar n=C3=BAmeros de %s d=C3=
=ADgitos" (objetivoDigitos.ToString("N0"))
       =20
        let digitosActuales =3D aActual.ToString().Length
        if digitosActuales &gt;=3D objetivoDigitos then
            printfn "   =E2=9C=85 Ya estamos en %s d=C3=ADgitos" (digitosAc=
tuales.ToString("N0"))
            []
