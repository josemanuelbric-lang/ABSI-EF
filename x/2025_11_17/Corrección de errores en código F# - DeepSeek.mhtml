            |&gt; Seq.filter (fun (_, c) -&gt; c =3D '1')
            |&gt; Seq.map (fun (i, _) -&gt; binario.Length - i - 1)
            |&gt; List.ofSeq
       =20
        let posicionesCeros =3D
            binario
            |&gt; Seq.mapi (fun i c -&gt; (i, c))
            |&gt; Seq.filter (fun (_, c) -&gt; c =3D '0')
            |&gt; Seq.map (fun (i, _) -&gt; binario.Length - i - 1)
            |&gt; List.ofSeq
       =20
        (binario, posicionesUnos, posicionesCeros)

    member this.EsPrimo (n: BigInteger) =3D
        if n &lt; 2I then false
        elif n =3D 2I then true
        elif n % 2I =3D 0I then false
        else
            let raiz =3D bigint (Math.Sqrt(float n))
            let mutable i =3D 3I
            let mutable esPrimo =3D true
           =20
            while i &lt;=3D raiz &amp;&amp; esPrimo do
                if n % i =3D 0I then
                    esPrimo &lt;- false
                i &lt;- i + 2I
            esPrimo

    member this.EncontrarGoldbachParaTermino (n: BigInteger) =3D
        if n &lt;=3D 2I || n % 2I &lt;&gt; 0I then
            failwith "Debe ser par &gt; 2"
       =20
        // Para n=C3=BAmeros de esta forma estructural, hay patrones espec=
=C3=ADficos
        if n =3D 2I then (2I, 2I)  // Caso especial
        elif n =3D 10I then (3I, 7I) // 3 + 7
        elif n =3D 42I then (5I, 37I) // 5 + 37
        elif n =3D 170I then (3I, 167I) // 3 + 167
        elif n =3D 682I then (3I, 679I) // Pero 679 no es primo, buscar otr=
a
        else
            // Algoritmo general para otros t=C3=A9rminos
            let mutable p =3D 3I
            let mutable encontrado =3D false
            let mutable resultado =3D (0I, 0I)
           =20
            while p &lt;=3D n / 2I &amp;&amp; not encontrado do
                let q =3D n - p
                if this.EsPrimo p &amp;&amp; this.EsPrimo q then
                    resultado &lt;- (p, q)
                    encontrado &lt;- true
                p &lt;- p + 2I
           =20
            if not encontrado then
                // Segundo intento con m=C3=A9todo m=C3=A1s robusto
                p &lt;- 2I
                while p &lt;=3D n / 2I &amp;&amp; not encontrado do
                    let q =3D n - p
                    if this.EsPrimo p &amp;&amp; this.EsPrimo q then
                        resultado &lt;- (p, q)
                        encontrado &lt;- true
                    p &lt;- if p =3D 2I then 3I else p + 2I
           =20
            if not encontrado then
                // =C3=9Altimo intento: buscar cualquier representaci=C3=B3=
n
                p &lt;- 2I
                while p &lt;=3D n / 2I do
                    let q =3D n - p
                    if this.EsPrimo p &amp;&amp; this.EsPrimo q then
                        resultado &lt;- (p, q)
                        encontrado &lt;- true
                    p &lt;- this.SiguientePrimo p
            resultado

    member private this.SiguientePrimo (n: BigInteger) =3D
        let mutable candidato =3D if n =3D 2I then 3I else n + 2I
        while not (this.EsPrimo candidato) do
            candidato &lt;- candidato + 2I
        candidato

