// CAZADOR DE PRIMOS INFINITO
// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D

type CazadorPrimosInfinito() =3D
    let mutable primoMasGrande =3D BigInteger.Zero
    let mutable terminoActual =3D 1
    let mutable aActual =3D BigInteger(2)
    let mutable inicioTiempo =3D DateTime.Now
    let mutable ultimoExponenteGuardado =3D 0
    let archivoResultados =3D "primos_gigantes.txt"

    // Crear archivo de resultados si no existe
    do
        if not (File.Exists(archivoResultados)) then
            File.WriteAllText(archivoResultados,=20
                "=3D=3D=3D REGISTRO DE PRIMOS GIGANTES =3D=3D=3D\n" +
                "Fecha de inicio: " + DateTime.Now.ToString() + "\n" +
                "Secuencia: a=E2=82=99 =3D 4a=E2=82=99=E2=82=8B=E2=82=81 + =
2\n" +
                "=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D\n\n")

    // Test de primalidad Miller-Rabin optimizado
    let esPrimoProbabilistico (n: BigInteger) (k: int) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            let mutable d =3D n - 1I
            let mutable r =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                r &lt;- r + 1
           =20
            let testWitness (a: BigInteger) =3D
                let mutable x =3D BigInteger.ModPow(a, d, n)
                if x =3D 1I || x =3D n - 1I then true
                else
                    let mutable continuar =3D true
                    let mutable resultado =3D false
                    for _ in 1 .. (r - 1) do
                        if continuar then
                            x &lt;- BigInteger.ModPow(x, 2I, n)
                            if x =3D n - 1I then
                                resultado &lt;- true
                                continuar &lt;- false
                    resultado
           =20
            let bases =3D [|2I; 3I; 5I; 7I; 11I; 13I; 17I; 19I; 23I; 29I; 3=
1I; 37I|]
            let mutable esPrimo =3D true
           =20
            for i in 0 .. (min (k - 1) (bases.Length - 1)) do
                if not (testWitness bases.[i]) then
                    esPrimo &lt;- false
           =20
            esPrimo

    // Guardar primo en archivo si es mayor que 2^N
    let guardarPrimoSiEsGrande (primo: BigInteger) (termino: int) =3D
        let mutable exponente =3D ultimoExponenteGuardado + 1
        let mutable debeGuardar =3D false
       =20
        // Encontrar el mayor 2^N que sea menor que el primo
        while BigInteger.Pow(2I, exponente) &lt;=3D primo do
            exponente &lt;- exponente + 1
       =20
        // Si encontramos un nuevo exponente m=C3=A1ximo, guardar
        if exponente - 1 &gt; ultimoExponenteGuardado then
            ultimoExponenteGuardado &lt;- exponente - 1
            debeGuardar &lt;- true
           =20
            let tiempoTranscurrido =3D DateTime.Now - inicioTiempo
            let registro =3D=20
                sprintf "\n=F0=9F=8E=AF NUEVO R=C3=89CORD: T=C3=A9rmino %d\=
n" termino +
                sprintf "=F0=9F=93=85 Fecha: %s\n" (DateTime.Now.ToString("=
yyyy-MM-dd HH:mm:ss")) +
                sprintf "=E2=8F=B1=EF=B8=8F  Tiempo transcurrido: %02d:%02d=
:%02d\n" tiempoTranscurrido.Hours tiempoTranscurrido.Minutes tiempoTranscur=
rido.Seconds +
                sprintf "=F0=9F=94=A2 Primo: %s\n" (primo.ToString()) +
                sprintf "=F0=9F=93=8F D=C3=ADgitos: %d\n" (primo.ToString()=
.Length) +
                sprintf "=E2=9A=A1 Mayor que: 2^%d =3D %s\n" ultimoExponent=
eGuardado (BigInteger.Pow(2I, ultimoExponenteGuardado).ToString()) +
                sprintf "%s\n" (String('=3D', 80))
           =20
            File.AppendAllText(archivoResultados, registro)
            printfn "=F0=9F=92=BE GUARDADO EN ARCHIVO: Primo mayor que 2^%d=
" ultimoExponenteGuardado
       =20
        debeGuardar

    // Mostrar progreso en consola
    let mostrarProgreso (primo: BigInteger) (termino: int) (esNuevoRecord: =
bool) =3D
        let tiempoTranscurrido =3D DateTime.Now - inicioTiempo
        let digitos =3D primo.ToString().Length
       =20
        if esNuevoRecord then
            printfn "\n=F0=9F=8E=89 =C2=A1NUEVO R=C3=89CORD! T=C3=A9rmino %=
d" termino
            printfn "   =F0=9F=94=A2 Primo: %s" (primo.ToString())
            printfn "   =F0=9F=93=8F D=C3=ADgitos: %d" digitos
            printfn "   =E2=8F=B1=EF=B8=8F  Tiempo: %02d:%02d:%02d"=20
                tiempoTranscurrido.Hours tiempoTranscurrido.Minutes tiempoT=
ranscurrido.Seconds
            printfn "   =E2=9A=A1 Mayor que: 2^%d" ultimoExponenteGuardado
            printfn "   %s" (String('~', 50))
        else
            printfn "=F0=9F=93=8A T=C3=A9rmino %6d | D=C3=ADgitos: %6d | Ti=
empo: %02d:%02d:%02d"=20
                termino digitos
                tiempoTranscurrido.Hours tiempoTranscurrido.Minutes tiempoT=
ranscurrido.Seconds

    // Buscar primos en el t=C3=A9rmino actual
    let buscarPrimosEnTermino (a_n: BigInteger) (termino: int) =3D
        let mutable primosEncontrados =3D []

        // Verificar si el t=C3=A9rmino actual es primo
        if esPrimoProbabilistico a_n 8 then
            primosEncontrados &lt;- a_n :: primosEncontrados

        // Verificar n-1 y n+1 (patr=C3=B3n com=C3=BAn en la secuencia)
        let nMenos1 =3D a_n - 1I
        let nMas1 =3D a_n + 1I
       =20
        if esPrimoProbabilistico nMenos1 6 then
            primosEncontrados &lt;- nMenos1 :: primosEncontrados
       =20
        if esPrimoProbabilistico nMas1 6 then
            primosEncontrados &lt;- nMas1 :: primosEncontrados

        primosEncontrados

    // Ejecuci=C3=B3n infinita
    member this.IniciarBusquedaInfinita() =3D
        printfn "=F0=9F=9A=80 CAZADOR DE PRIMOS INFINITO"
        printfn "%s" (String('=3D', 70))
        printfn "=F0=9F=94=8D Buscando primos m=C3=A1s grandes que 2^N usan=
do tu secuencia di=C3=A1dica"
        printfn "=F0=9F=92=BE Los resultados se guardan en: %s" archivoResu=
ltados
        printfn "=E2=8F=B0 Inicio: %s" (DateTime.Now.ToString("yyyy-MM-dd H=
H:mm:ss"))
        printfn "%s" (String('=3D', 70))
       =20
        // Escribir encabezado en archivo
        File.AppendAllText(archivoResultados,=20
            sprintf "\n=F0=9F=9A=80 INICIO DE B=C3=9ASQUEDA INFINITA\n" +
            sprintf "=F0=9F=93=85 %s\n" (DateTime.Now.ToString("yyyy-MM-dd =
HH:mm:ss")) +
            sprintf "%s\n" (String('=3D', 60)))
       =20
        let mutable continuar =3D true
       =20
        try
            while continuar do
                // Calcular t=C3=A9rmino actual: a=E2=82=99 =3D 4a=E2=82=99=
=E2=82=8B=E2=82=81 + 2
                if terminoActual =3D 1 then
                    aActual &lt;- BigInteger(2)
                else
                    aActual &lt;- 4I * aActual + 2I
               =20
                // Buscar primos en este t=C3=A9rmino
                let primosEncontrados =3D buscarPrimosEnTermino aActual ter=
minoActual
               =20
                for primo in primosEncontrados do
                    if primo &gt; primoMasGrande then
                        primoMasGrande &lt;- primo
                        let esNuevoRecord =3D guardarPrimoSiEsGrande primo =
terminoActual
                        mostrarProgreso primo terminoActual esNuevoRecord
               =20
                // Mostrar progreso cada 100 t=C3=A9rminos
                if terminoActual % 100 =3D 0 then
                    if primoMasGrande &gt; BigInteger.Zero then
                        mostrarProgreso primoMasGrande terminoActual false
                    else
                        let tiempoTranscurrido =3D DateTime.Now - inicioTie=
mpo
                        printfn "=F0=9F=93=8A T=C3=A9rmino %6d | Sin primos=
 encontrados a=C3=BAn | Tiempo: %02d:%02d:%02d"=20
                            terminoActual
                            tiempoTranscurrido.Hours tiempoTranscurrido.Min=
utes tiempoTranscurrido.Seconds
               =20
                terminoActual &lt;- terminoActual + 1
               =20
                // Peque=C3=B1a pausa para no saturar la CPU
                if terminoActual % 1000 =3D 0 then
                    System.Threading.Thread.Sleep(10)
                   =20
        with
        | ex -&gt;
            printfn "\n=E2=9D=8C ERROR: %s" ex.Message
            printfn "Stack trace: %s" ex.StackTrace
            File.AppendAllText(archivoResultados,=20
                sprintf "\n=E2=9D=8C ERROR DETENIDO: %s\n" ex.Message +
                sprintf "=F0=9F=93=85 %s\n" (DateTime.Now.ToString("yyyy-MM=
-dd HH:mm:ss")))

    // Mostrar resumen actual
    member this.MostrarResumen() =3D
        let tiempoTotal =3D DateTime.Now - inicioTiempo
        printfn "\n%s" (String('=3D', 70))
        printfn "=F0=9F=93=8A RESUMEN ACTUAL"
        printfn "%s" (String('=3D', 70))
        printfn "   =E2=80=A2 T=C3=A9rminos procesados: %d" (terminoActual =
- 1)
        printfn "   =E2=80=A2 Tiempo ejecuci=C3=B3n: %02d:%02d:%02d"=20
            tiempoTotal.Hours tiempoTotal.Minutes tiempoTotal.Seconds
        printfn "   =E2=80=A2 Primo m=C3=A1s grande: %s" (if primoMasGrande=
 &gt; BigInteger.Zero then primoMasGrande.ToString() else "Ninguno a=C3=BAn=
")
        printfn "   =E2=80=A2 D=C3=ADgitos del primo m=C3=A1s grande: %d" (=
if primoMasGrande &gt; BigInteger.Zero then primoMasGrande.ToString().Lengt=
h else 0)
        printfn "   =E2=80=A2 Mayor que: 2^%d" ultimoExponenteGuardado
        printfn "   =E2=80=A2 Archivo de resultados: %s" archivoResultados

