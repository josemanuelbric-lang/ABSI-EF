        if numeroPar &lt;=3D 2I || numeroPar % 2I &lt;&gt; 0I then
            failwith "El n=C3=BAmero debe ser par y mayor que 2"
       =20
        // Buscar primos hasta n/2 (por simetr=C3=ADa)
        let mitad =3D numeroPar / 2I
       =20
        let mutable p =3D 2I
        let mutable encontrado =3D false
        let mutable representacion =3D (0I, 0I)
       =20
        while p &lt;=3D mitad &amp;&amp; not encontrado do
            let q =3D numeroPar - p
           =20
            // Verificar si ambos son primos
            if esPrimoConocido p &amp;&amp; esPrimoConocido q then
                representacion &lt;- (p, q)
