    // Test de primalidad optimizado para n=C3=BAmeros grandes
    let esPrimoRapido (n: BigInteger) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            // Test Miller-Rabin r=C3=A1pido con bases peque=C3=B1as
            let mutable d =3D n - 1I
