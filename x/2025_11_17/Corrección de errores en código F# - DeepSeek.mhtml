// CAZADOR DE PRIMOS INFINITO - DESDE MERSENNE 136M
// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D

type CazadorPrimosInfinito() =3D
    let mutable primoMasGrande =3D BigInteger.Zero
    let mutable terminoActual =3D 1
    let mutable aActual =3D BigInteger(2)  // Comenzar desde el principio r=
eal
    let mutable inicioTiempo =3D DateTime.Now
    let mutable ultimoExponenteGuardado =3D 0
    let archivoResultados =3D "primos_gigantes_mersenne136m.txt"
    let mutable objetivoAlcanzado =3D false
    let exponenteObjetivo =3D 136279841

    // Crear archivo de resultados si no existe
    do
        if not (File.Exists(archivoResultados)) then
            File.WriteAllText(archivoResultados,=20
                "=3D=3D=3D REGISTRO DE PRIMOS GIGANTES - HACIA MERSENNE 136=
M =3D=3D=3D\n" +
                "Fecha de inicio: " + DateTime.Now.ToString() + "\n" +
                "Secuencia: a=E2=82=99 =3D 4a=E2=82=99=E2=82=8B=E2=82=81 + =
2\n" +
                "Objetivo: Alcanzar y superar 2^136,279,841 - 1 (41,024,320=
 d=C3=ADgitos)\n" +
                "=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D\n\n")

    // Funci=C3=B3n para calcular cu=C3=A1ntos t=C3=A9rminos necesitamos pa=
ra alcanzar el tama=C3=B1o objetivo
    let estimarTerminosParaObjetivo() =3D
        // Estimaci=C3=B3n aproximada: cada t=C3=A9rmino aproximadamente cu=
adruplica el tama=C3=B1o
        // Queremos alcanzar ~2^136279841, comenzando desde 2
        // log2(2^136279841 / 2) / log2(4) =E2=89=88 136279841 / 2 =E2=89=
=88 68 millones de t=C3=A9rminos
        let estimacion =3D float exponenteObjetivo / 2.0
        int estimacion

    // Avanzar r=C3=A1pidamente hasta cerca del objetivo
    let avanzarRapidamenteHaciaObjetivo() =3D
        printfn "=E2=9A=A1 Avanzando r=C3=A1pidamente hacia el tama=C3=B1o =
objetivo..."
        let terminosEstimados =3D estimarTerminosParaObjetivo()
        printfn "   =F0=9F=93=8A T=C3=A9rminos estimados necesarios: ~%,d" =
terminosEstimados
       =20
        // Avanzamos en bloques grandes hasta acercarnos al objetivo
        let bloqueSize =3D 1000
        let mutable bloquesCompletados =3D 0
       =20
        while aActual.ToString().Length &lt; 41024320 &amp;&amp; bloquesCom=
pletados &lt; 10000 do // L=C3=ADmite de seguridad
            // Aplicar la f=C3=B3rmula en bloques: a=E2=82=99 =3D 4a=E2=82=
=99=E2=82=8B=E2=82=81 + 2
            // Podemos optimizar usando: a=E2=82=99 =E2=89=88 4^k * a=E2=82=
=80 para k grande
            if bloquesCompletados % 100 =3D 0 then
                printfn "   =F0=9F=93=88 Bloque %,d | D=C3=ADgitos actuales=
: %,d" bloquesCompletados (aActual.ToString().Length)
           =20
            for i in 1 .. bloqueSize do
                aActual &lt;- 4I * aActual + 2I
                terminoActual &lt;- terminoActual + 1
           =20
            bloquesCompletados &lt;- bloquesCompletados + 1
           =20
            // Peque=C3=B1a pausa para no saturar
            if bloquesCompletados % 10 =3D 0 then
                System.Threading.Thread.Sleep(10)
       =20
        printfn "   =E2=9C=85 Avance r=C3=A1pido completado"
        printfn "   =F0=9F=93=8A T=C3=A9rmino actual: %,d" terminoActual
        printfn "   =F0=9F=94=A2 D=C3=ADgitos actuales: %,d" (aActual.ToStr=
ing().Length)
        objetivoAlcanzado &lt;- aActual.ToString().Length &gt;=3D 41024320

    // Test de primalidad Miller-Rabin optimizado para n=C3=BAmeros gigante=
s
    let esPrimoProbabilistico (n: BigInteger) (k: int) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            let mutable d =3D n - 1I
            let mutable r =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                r &lt;- r + 1
           =20
            let random =3D Random()
            let testWitness (a: BigInteger) =3D
                let mutable x =3D BigInteger.ModPow(a, d, n)
                if x =3D 1I || x =3D n - 1I then true
                else
                    let mutable continuar =3D true
                    let mutable resultado =3D false
                    for _ in 1 .. (r - 1) do
                        if continuar then
                            x &lt;- BigInteger.ModPow(x, 2I, n)
                            if x =3D n - 1I then
                                resultado &lt;- true
                                continuar &lt;- false
                    resultado
           =20
            let mutable esPrimo =3D true
            let mutable testsRealizados =3D 0
           =20
            while testsRealizados &lt; k &amp;&amp; esPrimo do
                // Para n=C3=BAmeros muy grandes, usar bases fijas es m=C3=
=A1s eficiente
                let bases =3D [|2I; 3I; 5I; 7I; 11I; 13I; 17I; 19I; 23I; 29=
I|]
                if testsRealizados &lt; bases.Length &amp;&amp; bases.[test=
sRealizados] &lt; n then
                    if not (testWitness bases.[testsRealizados]) then
                        esPrimo &lt;- false
                else
                    // Generar testigo aleatorio para tests adicionales
                    let a =3D=20
                        let bytes =3D n.ToByteArray()
                        let randomBytes =3D Array.zeroCreate (min bytes.Len=
gth 8) // Limitar tama=C3=B1o
                        random.NextBytes(randomBytes)
                        let randomBigInt =3D BigInteger(randomBytes)
                        let absRandom =3D BigInteger.Abs(randomBigInt)
                        2I + (absRandom % (n - 4I))
                   =20
                    if not (testWitness a) then
                        esPrimo &lt;- false
               =20
                testsRealizados &lt;- testsRealizados + 1
           =20
            esPrimo

    // Versi=C3=B3n m=C3=A1s r=C3=A1pida para screening inicial
    let esPosiblePrimo (n: BigInteger) =3D
        if n &lt; 2I then false
        elif n =3D 2I then true
        elif n % 2I =3D 0I then false
        else
            // Test r=C3=A1pido con bases peque=C3=B1as
            let bases =3D [|2I; 3I; 5I; 7I; 11I|]
            let mutable d =3D n - 1I
            let mutable r =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                r &lt;- r + 1
           =20
            let testWitness (a: BigInteger) =3D
                try
                    let mutable x =3D BigInteger.ModPow(a, d, n)
                    if x =3D 1I || x =3D n - 1I then true
                    else
                        let mutable continuar =3D true
                        let mutable resultado =3D false
                        for _ in 1 .. (r - 1) do
                            if continuar then
                                x &lt;- BigInteger.ModPow(x, 2I, n)
                                if x =3D n - 1I then
                                    resultado &lt;- true
                                    continuar &lt;- false
                        resultado
                with
                | _ -&gt; false // En caso de error en ModPow
           =20
            let mutable esPrimo =3D true
            let mutable i =3D 0
           =20
            while i &lt; min 2 bases.Length &amp;&amp; esPrimo do
                if bases.[i] &lt; n then
                    if not (testWitness bases.[i]) then
                        esPrimo &lt;- false
                i &lt;- i + 1
           =20
            esPrimo

    // Guardar primo en archivo
    let guardarPrimo (primo: BigInteger) (termino: int) (esRecord: bool) =
=3D
        let tiempoTranscurrido =3D DateTime.Now - inicioTiempo
        let digitos =3D primo.ToString().Length
       =20
        let registro =3D=20
            if esRecord then
                sprintf "\n=F0=9F=8E=AF NUEVO R=C3=89CORD: T=C3=A9rmino %d\=
n" termino +
                sprintf "=F0=9F=93=85 Fecha: %s\n" (DateTime.Now.ToString("=
yyyy-MM-dd HH:mm:ss")) +
                sprintf "=E2=8F=B1=EF=B8=8F  Tiempo: %d d=C3=ADas, %02d:%02=
d:%02d\n" tiempoTranscurrido.Days tiempoTranscurrido.Hours tiempoTranscurri=
do.Minutes tiempoTranscurrido.Seconds +
                sprintf "=F0=9F=94=A2 D=C3=ADgitos: %,d\n" digitos +
                sprintf "=F0=9F=93=8F Tama=C3=B1o: ~2^%.1f\n" (Math.Log(flo=
at digitos * 3.321928094887362) ) + // log2(10) =E2=89=88 3.3219
                sprintf "%s\n" (String('=3D', 70))
            else
                sprintf "\n=F0=9F=93=8C Primo encontrado: T=C3=A9rmino %d\n=
" termino +
                sprintf "=F0=9F=93=85 Fecha: %s\n" (DateTime.Now.ToString("=
yyyy-MM-dd HH:mm:ss")) +
                sprintf "=F0=9F=94=A2 D=C3=ADgitos: %,d\n" digitos +
                sprintf "%s\n" (String('~', 50))
       =20
        File.AppendAllText(archivoResultados, registro)
       =20
        if esRecord then
            let archivoPrimoCompleto =3D $"primo_termino_{termino}_digitos_=
{digitos}.txt"
            try
                File.WriteAllText(archivoPrimoCompleto, primo.ToString())
                printfn "=F0=9F=92=BE Guardado completo en: %s" archivoPrim=
oCompleto
            with
            | ex -&gt;=20
                printfn "=E2=9A=A0=EF=B8=8F  No se pudo guardar el n=C3=BAm=
ero completo: %s" ex.Message

    // Buscar primos en el t=C3=A9rmino actual
    let buscarPrimosEnTermino (a_n: BigInteger) (termino: int) =3D
        let mutable primosEncontrados =3D []

        // Solo buscar primos cuando estemos cerca o hayamos alcanzado el o=
bjetivo
        if a_n.ToString().Length &gt;=3D 41024300 then // Cerca del objetiv=
o
            let candidatos =3D [|
                a_n - 3I; a_n - 1I; a_n + 1I; a_n + 3I;
            |]

            for candidato in candidatos do
                if candidato &gt; 0I then
                    let digitosCandidato =3D candidato.ToString().Length
                    // Solo probar candidatos del tama=C3=B1o correcto
                    if digitosCandidato &gt;=3D 41024300 then
                        if esPosiblePrimo candidato then
                            printfn "   =F0=9F=94=8D Probando candidato de =
%,d d=C3=ADgitos..." digitosCandidato
                            if esPrimoProbabilistico candidato 3 then
                                primosEncontrados &lt;- candidato :: primos=
Encontrados
                                printfn "   =F0=9F=8E=AF =C2=A1Posible prim=
o encontrado! %,d d=C3=ADgitos" digitosCandidato

        primosEncontrados

    // Ejecuci=C3=B3n principal
    member this.IniciarBusquedaInfinita() =3D
        printfn "=F0=9F=9A=80 CAZADOR DE PRIMOS INFINITO - HACIA MERSENNE 1=
36M"
        printfn "%s" (String('=3D', 80))
        printfn "=F0=9F=94=8D Objetivo: Alcanzar y superar 2^136,279,841 - =
1 (41,024,320 d=C3=ADgitos)"
        printfn "=F0=9F=92=BE Archivo de resultados: %s" archivoResultados
        printfn "=E2=8F=B0 Inicio: %s" (DateTime.Now.ToString("yyyy-MM-dd H=
H:mm:ss"))
        printfn "%s" (String('=3D', 80))
       =20
        File.AppendAllText(archivoResultados,=20
            sprintf "\n=F0=9F=9A=80 INICIO DE B=C3=9ASQUEDA\n" +
            sprintf "=F0=9F=93=85 %s\n" (DateTime.Now.ToString("yyyy-MM-dd =
HH:mm:ss")) +
            sprintf "=F0=9F=8E=AF Objetivo: 2^136,279,841 - 1 (41,024,320 d=
=C3=ADgitos)\n" +
            sprintf "%s\n" (String('=3D', 60)))
       =20
        // Fase 1: Avance r=C3=A1pido hacia el objetivo
        avanzarRapidamenteHaciaObjetivo()
       =20
        if not objetivoAlcanzado then
            printfn "=E2=9A=A0=EF=B8=8F  No se alcanz=C3=B3 el tama=C3=B1o =
objetivo en la fase r=C3=A1pida"
            printfn "   Continuando con b=C3=BAsqueda normal..."
       =20
        printfn "\n=F0=9F=8E=AF INICIANDO B=C3=9ASQUEDA DE PRIMOS GIGANTES.=
.."
        printfn "%s" (String('=3D', 60))
       =20
        let mutable continuar =3D true
       =20
        try
            while continuar do
                // Calcular siguiente t=C3=A9rmino
                aActual &lt;- 4I * aActual + 2I
               =20
                // Mostrar progreso
                if terminoActual % 10 =3D 0 then
                    let digitos =3D aActual.ToString().Length
                    let tiempoTranscurrido =3D DateTime.Now - inicioTiempo
                    printfn "=F0=9F=93=8A T=C3=A9rmino %,d | D=C3=ADgitos: =
%,d | Tiempo: %d d, %02d:%02d:%02d"=20
                        terminoActual digitos
                        tiempoTranscurrido.Days tiempoTranscurrido.Hours ti=
empoTranscurrido.Minutes tiempoTranscurrido.Seconds
               =20
                // Buscar primos (cada 5 t=C3=A9rminos cuando estemos en el=
 rango objetivo)
                if aActual.ToString().Length &gt;=3D 41024300 &amp;&amp; te=
rminoActual % 5 =3D 0 then
                    let primosEncontrados =3D buscarPrimosEnTermino aActual=
 terminoActual
                   =20
                    for primo in primosEncontrados do
                        let esRecord =3D primo &gt; primoMasGrande
                        if esRecord then
                            primoMasGrande &lt;- primo
                            ultimoExponenteGuardado &lt;- max ultimoExponen=
teGuardado (primo.ToString().Length)
                        guardarPrimo primo terminoActual esRecord
               =20
                terminoActual &lt;- terminoActual + 1
               =20
                // Pausa ocasional
                if terminoActual % 100 =3D 0 then
                    System.Threading.Thread.Sleep(50)
                   =20
        with
        | ex -&gt;
            printfn "\n=E2=9D=8C ERROR: %s" ex.Message
            File.AppendAllText(archivoResultados,=20
                sprintf "\n=E2=9D=8C ERROR: %s\n" ex.Message +
                sprintf "=F0=9F=93=85 %s\n" (DateTime.Now.ToString("yyyy-MM=
-dd HH:mm:ss")))

    member this.MostrarResumen() =3D
        let tiempoTotal =3D DateTime.Now - inicioTiempo
        printfn "\n%s" (String('=3D', 80))
        printfn "=F0=9F=93=8A RESUMEN - B=C3=9ASQUEDA HACIA MERSENNE 136M"
        printfn "%s" (String('=3D', 80))
        printfn "   =E2=80=A2 T=C3=A9rminos procesados: %,d" terminoActual
        printfn "   =E2=80=A2 Tiempo ejecuci=C3=B3n: %d d=C3=ADas, %02d:%02=
d:%02d"=20
            tiempoTotal.Days tiempoTotal.Hours tiempoTotal.Minutes tiempoTo=
tal.Seconds
        printfn "   =E2=80=A2 D=C3=ADgitos actuales: %,d" (aActual.ToString=
().Length)
        printfn "   =E2=80=A2 Primo m=C3=A1s grande: %s"=20
            (if primoMasGrande &gt; BigInteger.Zero then=20
                sprintf "%,d d=C3=ADgitos" (primoMasGrande.ToString().Lengt=
h)=20
             else "Ninguno encontrado")
        printfn "   =E2=80=A2 Objetivo alcanzado: %s" (if objetivoAlcanzado=
 then "=E2=9C=85 S=C3=AD" else "=E2=9D=8C No")

