open System
open System.Numerics
open System.IO

// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D
// CAZADOR DE PRIMOS INFINITO - DESDE MERSENNE 136M
// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D

type CazadorPrimosInfinito() =3D
    let mutable primoMasGrande =3D BigInteger.Zero
    let mutable terminoActual =3D 1
    let mutable aActual =3D BigInteger.Zero
    let mutable inicioTiempo =3D DateTime.Now
    let mutable ultimoExponenteGuardado =3D 136279841
    let archivoResultados =3D "primos_gigantes_mersenne136m.txt"

    // Crear archivo de resultados si no existe
    do
        if not (File.Exists(archivoResultados)) then
            File.WriteAllText(archivoResultados,=20
                "=3D=3D=3D REGISTRO DE PRIMOS GIGANTES - DESDE MERSENNE 136=
M =3D=3D=3D\n" +
                "Fecha de inicio: " + DateTime.Now.ToString() + "\n" +
                "Secuencia: a=E2=82=99 =3D 4a=E2=82=99=E2=82=8B=E2=82=81 + =
2\n" +
                "Punto de partida: Cerca de 2^136,279,841 - 1 (41,024,320 d=
=C3=ADgitos)\n" +
                "=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D\n\n")

        // Inicializar desde un punto cercano al primo Mersenne conocido
        // Usamos 2^136,279,840 como punto de partida (ligeramente menor qu=
e el primo conocido)
        let baseInicial =3D BigInteger.Pow(2I, 136279840)
        aActual &lt;- baseInicial
        primoMasGrande &lt;- baseInicial

    // Test de primalidad Miller-Rabin optimizado para n=C3=BAmeros gigante=
s
    let esPrimoProbabilistico (n: BigInteger) (k: int) =3D
        if n &lt; 2I then false
        elif n =3D 2I || n =3D 3I then true
        elif n % 2I =3D 0I then false
        else
            let mutable d =3D n - 1I
            let mutable r =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                r &lt;- r + 1
           =20
            let random =3D Random()
            let testWitness (a: BigInteger) =3D
                let mutable x =3D BigInteger.ModPow(a, d, n)
                if x =3D 1I || x =3D n - 1I then true
                else
                    let mutable continuar =3D true
                    let mutable resultado =3D false
                    for _ in 1 .. (r - 1) do
                        if continuar then
                            x &lt;- BigInteger.ModPow(x, 2I, n)
                            if x =3D n - 1I then
                                resultado &lt;- true
                                continuar &lt;- false
                    resultado
           =20
            let mutable esPrimo =3D true
            let mutable testsRealizados =3D 0
           =20
            while testsRealizados &lt; k &amp;&amp; esPrimo do
                // Generar testigo aleatorio entre 2 y n-2
                let a =3D=20
                    let bytes =3D n.ToByteArray()
                    let randomBytes =3D Array.zeroCreate bytes.Length
                    random.NextBytes(randomBytes)
                    let randomBigInt =3D BigInteger(randomBytes)
                    let absRandom =3D BigInteger.Abs(randomBigInt)
                    2I + (absRandom % (n - 4I))
               =20
                if not (testWitness a) then
                    esPrimo &lt;- false
               =20
                testsRealizados &lt;- testsRealizados + 1
           =20
            esPrimo

    // Versi=C3=B3n m=C3=A1s r=C3=A1pida para screening inicial (menos test=
s)
    let esPosiblePrimo (n: BigInteger) =3D
        if n &lt; 2I then false
        elif n =3D 2I then true
        elif n % 2I =3D 0I then false
        else
            // Test r=C3=A1pido con bases peque=C3=B1as
            let bases =3D [|2I; 3I; 5I; 7I; 11I; 13I; 17I; 19I|]
            let mutable d =3D n - 1I
            let mutable r =3D 0
            while d % 2I =3D 0I do
                d &lt;- d / 2I
                r &lt;- r + 1
           =20
            let testWitness (a: BigInteger) =3D
                let mutable x =3D BigInteger.ModPow(a, d, n)
                if x =3D 1I || x =3D n - 1I then true
                else
                    let mutable continuar =3D true
                    let mutable resultado =3D false
                    for _ in 1 .. (r - 1) do
                        if continuar then
                            x &lt;- BigInteger.ModPow(x, 2I, n)
                            if x =3D n - 1I then
                                resultado &lt;- true
                                continuar &lt;- false
                    resultado
           =20
            let mutable esPrimo =3D true
            let mutable i =3D 0
           =20
            while i &lt; min 3 bases.Length &amp;&amp; esPrimo do
                if bases.[i] &lt; n then
                    if not (testWitness bases.[i]) then
                        esPrimo &lt;- false
                i &lt;- i + 1
           =20
            esPrimo

    // Guardar primo en archivo si es mayor que el r=C3=A9cord actual
    let guardarPrimoSiEsGrande (primo: BigInteger) (termino: int) =3D
        let mutable exponente =3D ultimoExponenteGuardado
        let mutable debeGuardar =3D false
       =20
        // Encontrar el mayor 2^N que sea menor que el primo
        while BigInteger.Pow(2I, exponente + 1) &lt;=3D primo do
            exponente &lt;- exponente + 1
