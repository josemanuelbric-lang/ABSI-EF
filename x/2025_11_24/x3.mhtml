<span class=3D"hljs-comment"># Estado Positivo (Existencia)</span>
psi_1 =3D np.array([<span class=3D"hljs-number">1</span>, <span class=3D"hl=
js-number">0</span>]) =20
<span class=3D"hljs-comment"># Estado Negativo (Anti-Existencia)</span>
psi_2 =3D np.array([<span class=3D"hljs-number">0</span>, <span class=3D"hl=
js-number">1</span>]) =20

<span class=3D"hljs-comment"># --- 2. Operador de Emergencia de Signo (Obse=
rvable) ---</span>
<span class=3D"hljs-comment"># Este operador mide el Signo (+1 o -1). </spa=
n>
<span class=3D"hljs-comment"># Corresponde a la matriz que tiene autovalore=
s +1 y -1.</span>
A_operador =3D np.array([[<span class=3D"hljs-number">1</span>, <span class=
=3D"hljs-number">0</span>],=20
                       [<span class=3D"hljs-number">0</span>, -<span class=
=3D"hljs-number">1</span>]])=20

<span class=3D"hljs-comment"># --- 3. Definici=C3=B3n de la Matriz de Densi=
dad (Mezcla) ---</span>
p_1 =3D <span class=3D"hljs-number">0.5</span>  <span class=3D"hljs-comment=
"># 50% de probabilidad de ser Positivo</span>
p_2 =3D <span class=3D"hljs-number">0.5</span>  <span class=3D"hljs-comment=
"># 50% de probabilidad de ser Negativo</span>

<span class=3D"hljs-comment"># rho =3D p1 * |psi1&gt;&lt;psi1| + p2 * |psi2=
&gt;&lt;psi2|</span>
rho_mezcla =3D p_1 * np.outer(psi_1, psi_1) + p_2 * np.outer(psi_2, psi_2)

<span class=3D"hljs-comment"># El resultado para esta mezcla 50/50 es la ma=
triz diagonal con 0.5 en la diagonal:</span>
<span class=3D"hljs-comment"># rho_mezcla =3D [[0.5, 0.0], </span>
<span class=3D"hljs-comment">#               [0.0, 0.5]]</span>

<span class=3D"hljs-comment"># --- 4. Medici=C3=B3n Colectiva (Energ=C3=ADa=
