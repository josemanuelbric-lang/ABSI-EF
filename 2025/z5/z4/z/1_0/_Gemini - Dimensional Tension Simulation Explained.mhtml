G_munu =3D IndexedBase(<span class=3D"hljs-string">'G'</span>, shape=3D(<sp=
an class=3D"hljs-number">4</span>, <span class=3D"hljs-number">4</span>)) <=
span class=3D"hljs-comment"># M=C3=A9trica de la Tensi=C3=B3n (G_munu)</spa=
n>
S_munu =3D IndexedBase(<span class=3D"hljs-string">'S'</span>, shape=3D(<sp=
an class=3D"hljs-number">4</span>, <span class=3D"hljs-number">4</span>)) <=
span class=3D"hljs-comment"># Tensor de Tensi=C3=B3n (S_munu)</span>
R_munu =3D IndexedBase(<span class=3D"hljs-string">'R'</span>, shape=3D(<sp=
an class=3D"hljs-number">4</span>, <span class=3D"hljs-number">4</span>)) <=
span class=3D"hljs-comment"># Tensor de Ricci (R_munu)</span>
R =3D symbols(<span class=3D"hljs-string">'R'</span>)                      =
  <span class=3D"hljs-comment"># Escalar de Ricci (R)</span>

<span class=3D"hljs-comment"># S=C3=ADmbolos de Christoffel (Gamma)</span>
Gamma_munu_alpha =3D IndexedBase(<span class=3D"hljs-string">'Gamma'</span>=
