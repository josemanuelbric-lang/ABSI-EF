=3D"code-container formatted ng-tns-c1594367902-84">; BTreeFuncional_Mult e=
n Assembler x86_64
; Entradas: RDI =3D a, RSI =3D b
; Salida: RAX =3D resultado

section .text
global btree_functional_mult_asm

btree_functional_mult_asm:
    xor rax, rax            ; result =3D 0
    test rsi, rsi           ; =C2=BFb =3D=3D 0?
    jz .done
    test rdi, rdi           ; =C2=BFa =3D=3D 0?
    jz .done

.loop:
    test rsi, rsi           ; =C2=BFb =3D=3D 0?
    jz .done

    ; --- OPTIMIZACI=C3=93N: PODA DE =C3=81RBOL (Axioma de Distancia) ---
    ; Verifica si b es una cadena de unos: b &amp; (b + 1) =3D=3D 0
    mov rdx, rsi
    add rdx, 1
    test rdx, rsi
    jnz .navigate           ; Si no es cadena de unos, navega el =C3=A1rbol

    ; Si es cadena de unos, aplicamos (a &lt;&lt; profundidad) - a
    bsr rcx, rsi            ; Encontrar MSB (posicion del bit m=C3=A1s alto=
)
    add rcx, 1              ; profundidad =3D msb + 1
    mov r8, rdi
    shl r8, cl              ; a &lt;&lt; profundidad
    sub r8, rdi             ; (a &lt;&lt; profundidad) - a
    add rax, r8             ; result +=3D r8
    jmp .done               ; Poda completa, terminar

.navigate:
    ; --- NAVEGACI=C3=93N: BUSCAR NODO L ---
    bsr rcx, rsi            ; Encontrar MSB de b
    mov r8, rdi
    shl r8, cl              ; r8 =3D a &lt;&lt; msb
    add rax, r8             ; result +=3D r8

    ; --- NAVEGACI=C3=93N: MOVERSE AL RESIDUO (Nodo R) ---
    mov rdx, 1
    shl rdx, cl             ; 1 &lt;&lt; msb
    xor rsi, rdx            ; b =3D b ^ (1 &lt;&lt; msb) (limpiar bit proce=
sado)
    jmp .loop

.done:
    ret
</code></pre><!----></div></div></div><!----><!----></code-block><!----><!-=
